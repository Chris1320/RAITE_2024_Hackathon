type Test = record { id : principal; item : blob };
service : () -> {
  getAllImages : () -> (vec Test) query;
  getImage : (principal) -> (opt Test) query;
  greet : (text) -> (text) query;
  login_user : (text) -> (text);
  register_user : (text) -> (text);
  retrieve_profile : (text) -> (text) query;
